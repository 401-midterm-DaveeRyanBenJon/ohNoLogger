generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model errevents {
  id        Int     @id @default(autoincrement())
  message   String?
  name      String?
  datetime  String?
  stack     String?
  clientid  String?
  userparam String?
  usernote  String?
}
